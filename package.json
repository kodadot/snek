{
  "name": "@kodadot1/snek",
  "version": "0.0.4",
  "private": true,
  "scripts": {
    "build": "rm -rf lib && tsc",
    "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
    "db:migrate": "npx squid-typeorm-migration apply",
    "processor:start": "node lib/processor.js",
    "query-node:start": "squid-graphql-server --subscriptions --max-root-fields 50 --dumb-cache in-memory --dumb-cache-ttl 1000 --dumb-cache-size 100 --dumb-cache-max-age 1000",
    "test": "vitest --run --dir tests"
  },
  "dependencies": {
    "@kodadot1/minipfs": "^0.1.1-rc.1",
    "@subsquid/archive-registry": "1.0.18",
    "@subsquid/cli": "^2.0.1",
    "@subsquid/graphql-server": "^3.3.0",
    "@subsquid/ss58": "^0.1.2",
    "@subsquid/substrate-processor": "2.2.0",
    "@subsquid/typeorm-migration": "0.1.4",
    "@subsquid/typeorm-store": "0.2.0",
    "axios": "^1.1.3",
    "dotenv": "^16.0.3",
    "md5": "^2.3.0",
    "nanoid": "3.3.4",
    "pg": "^8.8.0",
    "signale": "^1.4.0",
    "type-graphql": "^1.2.0-rc.1",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@subsquid/substrate-metadata-explorer": "1.0.9",
    "@subsquid/substrate-typegen": "2.0.0",
    "@subsquid/typeorm-codegen": "0.3.1",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.11.9",
    "@types/signale": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-type-graphql": "^0.3.2",
    "typescript": "~4.8.4",
    "vitest": "^0.25.2"
  }
}
